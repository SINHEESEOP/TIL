{
    "title": "Today I Learned",
    "description": "개발 학습 노트",
    "author": "SINHEESEOP",
    "language": "ko",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "highlight",
        "search",
        "theme-default",
        "expandable-chapters",
        "copy-code-button",
        "hints",
        "flexible-alerts"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "flexible-alerts": {
            "style": "callout",
            "comment": {
                "label": "Comment",
                "icon": "fa fa-comments",
                "className": "info"
            }
        }
    },
    "styles": {
        "website": "styles/website.css"
    },
    "pdf": {
        "pageNumbers": true,
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
            "right": 62,
            "left": 62,
            "top": 36,
            "bottom": 36
        }
    }
} 